sp {propose*initialize-tanksoar-default
   (state <s> ^superstate tanksoar
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-tanksoar-default)
}

sp {apply*initialize-tanksoar-default
   (state <s> ^operator <op>)
   (<op> ^name initialize-tanksoar-default)
-->
   (<s> ^name tanksoar)
}

